{
  "name": "steamgriddb-manager",
  "version": "0.0.3",
  "description": "Easily find and download new grid images for your Steam games.",
  "main": "main.js",
  "author": "SteamGridDB.com",
  "scripts": {
    "start": "electron .",
    "build": "webpack --mode development",
    "run": "webpack --mode development && electron .",
    "pack": "build --dir",
    "build": "build",
    "publish": "electron-builder --publish onTag",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.steamgriddb.manager",
    "productName": "SteamGridDB Manager",
    "copyright": "SteamGridDB.com",
    "directories": {
      "buildResources": "assets/icons"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false
    },
    "publish": {
      "provider": "s3",
      "endpoint": "http://192.168.0.18:9000",
      "bucket": "sgdb-manager"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "crc": "^3.8.0",
    "css-loader": "^2.1.1",
    "electron": "^4.2.8",
    "electron-builder": "^20.44.4",
    "electron-builder-squirrel-windows": "^20.44.0",
    "electron-packager": "^13.1.1",
    "electron-vibrancy": "^0.1.3",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "prop-types": "^15.7.2",
    "pubsub-js": "^1.7.0",
    "query-string": "^6.8.1",
    "react": "^15.6.1",
    "react-desktop": "^0.3.9",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.3.1",
    "react-uwp": "^1.2.31",
    "steam-id-convertor": "^1.0.1",
    "steamid": "^1.1.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@node-steam/vdf": "^2.0.1",
    "blizzard-product-parser": "^1.0.1",
    "electron-store": "^4.0.0",
    "electron-updater": "^4.1.2",
    "js-yaml": "^3.13.1",
    "jsonminify": "^0.4.1",
    "metrohash": "^2.6.0",
    "promise-reflect": "^1.1.0",
    "promise-settle": "^0.3.0",
    "react-motion": "^0.5.2",
    "react-transition-group": "^1.2.1",
    "steam-shortcut-editor": "^3.1.1",
    "steamgriddb": "^1.3.0",
    "winreg": "^1.2.4"
  }
}
