{
  "name": "steamgriddb-manager",
  "version": "0.0.1",
  "description": "Easily find and download new grid images for your Steam games.",
  "main": "main.js",
  "author": "SteamGridDB.com",
  "scripts": {
    "start": "electron .",
    "build": "webpack --mode development",
    "run": "webpack --mode development && electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "comnpm",
    "productName": "SteamGridDB Manager",
    "copyright": "SteamGridDB.com",
    "directories": {
      "buildResources": "assets/icons"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "crc": "^3.8.0",
    "css-loader": "^2.1.1",
    "electron": "^4.1.3",
    "electron-builder": "^20.39.0",
    "electron-builder-squirrel-windows": "^20.39.0",
    "electron-packager": "^13.1.1",
    "electron-rebuild": "^1.8.4",
    "electron-vibrancy": "^0.1.3",
    "prop-types": "^15.7.2",
    "pubsub-js": "^1.7.0",
    "query-string": "^6.4.2",
    "react": "^15.6.1",
    "react-desktop": "^0.3.9",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.3.1",
    "react-uwp": "^1.2.31",
    "steam-id-convertor": "^1.0.1",
    "steamid": "^1.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@node-steam/vdf": "^2.0.1",
    "electron-store": "^4.0.0",
    "steam-shortcut-editor": "^3.1.1",
    "steamgriddb": "^1.0.1",
    "winreg": "^1.2.4"
  }
}
